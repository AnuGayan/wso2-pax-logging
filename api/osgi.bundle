Bundle-Activator: org.ops4j.pax.logging.internal.Activator

Private-Package: org.apache.commons.logging.internal, \
                 org.apache.juli.logging.internal, \
                 org.apache.log4j.internal, \
                 org.ops4j.pax.logging.slf4j, \
                 org.slf4j.impl, \
                 org.ops4j.pax.logging.internal

Export-Package: org.ops4j.pax.logging; version=${pom.version}; provider=paxlogging,\
                org.osgi.service.log; version=1.3, \
                org.apache.juli.logging;version=1.0; provider=paxlogging, \
                org.apache.commons.logging;version=1.0.4; provider=paxlogging, \
