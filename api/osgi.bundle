                org.apache.avalon.framework.logger; version=4.3; provider=paxlogging

Import-Package: org.osgi.framework;version="[1.0.0, 2.0.0)", \
                org.osgi.util.tracker;version="[1.0.0, 2.0.0)", \
                org.ops4j.pax.logging;version="[0.9.5, 2.0.0)", \
                org.ops4j.pax.logging.avalon;version="[0.9.5, 2.0.0)", \
                org.osgi.service.event; version="[1.0.0, 2.0.0)", \
                org.osgi.service.log; version="[1.3.0, 2.0.0)", \
                org.apache.log; resolution:=optional, \
                javax.xml.parsers, \
                org.w3c.dom

